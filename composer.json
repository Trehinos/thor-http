  "description": "Une définition d'un serveur HTTP avec routage, sécurité et cycle Request->Controller->Response",
  "minimum-stability": "stable",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Thor\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Sébastien GELDREICH",
      "email": "trehinos@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "trehinos/thor-common": "1.*",
    "trehinos/thor-process": "1.*",
    "trehinos/thor-pdo-extension": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.*"
  }
}
